# Timezone
TZ=Asia/Baghdad

# --- Redis ---
# Set a strong password and uncomment the redis --requirepass command if you want auth.
REDIS_PASSWORD=YourStrongRedisPass123
REDIS_HOST_PORT=6379

# --- MySQL (only if enabled) ---
MYSQL_ROOT_PASSWORD=RootPass_ChangeMe
MYSQL_DATABASE=smartcar
MYSQL_USER=smartcar_user
MYSQL_PASSWORD=UserPass_ChangeMe
# Publish to host (optional). If you leave commented, MySQL is only reachable from other containers.
MYSQL_PUBLISH_PORT=3306

# --- phpMyAdmin (only if enabled) ---
# Bound to 127.0.0.1 for safety; open reverse-proxy if needed.
PHPMYADMIN_HOST_PORT=8081
